<p id="notice"><%= notice %></p>

<h1><%= @place.name %></h1>
<p><%= @place.status %></p>

<p><%= @place.street %></p>
<p><%= "#{@place.zip} #{@place.city}" %></p>


<iframe
width="600"
height="450"
frameborder="0" style="border:0"
src="https://www.google.com/maps/embed/v1/place?key=AIzaSyBCS4BFI4dWV6COyYTteo8UFhBMWdOTxpI
    &q=<%= "#{@place.name} #{@place.street} #{@place.city}".parameterize %>" allowfullscreen>
</iframe>